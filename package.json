{
  "name": "codeharbor-web-editor",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A production-ready web-based code editor with Monaco Editor, multi-language support, and modern development tools",
  "keywords": ["code-editor", "monaco-editor", "react", "typescript", "vite", "web-ide"],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,css,md}",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/jszip": "^3.4.1",
